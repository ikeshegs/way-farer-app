{
  "name": "way-farer-app",
  "version": "1.0.0",
  "description": "WayFarer is a public bus transportation booking App.",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha build/src/tests/*.js --recursive --require @babel/register --timeout 50000 --exit || true",
    "test-watch": "nodemon --exec \"npm test\"",
    "start": "node build/src/index.js",
    "dev-start": "nodemon --exec babel-node server/src/index.js",
    "build": "babel server --out-dir build",
    "serve": "node build/index.js",
    "clean": "rm -rf build && mkdir build",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "generate-lcov": "nyc report --reporter=text-lcov >lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikeshegs/way-farer-app.git"
  },
  "author": "Ikechukwu Okoro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ikeshegs/way-farer-app/issues"
  },
  "homepage": "https://github.com/ikeshegs/way-farer-app#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "codeclimate-test-reporter": "^0.5.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "mocha": "^6.1.4",
    "moment": "^2.24.0",
    "nyc": "^14.1.1",
    "regenerator-runtime": "^0.13.2",
    "uuid": "^3.3.2"
  }
}